{
  "name": "codetree-backend",
  "scripts": {
    "lint": "eslint --fix src",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "start": "npm run build && node -r dotenv/config build/server.js dotenv_config_path=environments/.env.production",
    "start:staging": "npm run build && node -r dotenv/config build/server.js dotenv_config_path=environments/.env.development",
    "start:deploy": "npm run build && node -r dotenv/config build/server.js dotenv_config_path=.env",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node -r dotenv/config src/server.ts dotenv_config_path=environments/.env.development",
    "pm2:restart:staging": "npx pm2 restart staging",
    "pm2:restart:prod": "npx pm2 restart prod",
    "pm2:start:staging": "npx pm2 --name staging start npm -- run 'start:staging'",
    "pm2:start:prod": "npx pm2 --name prod start npm -- run 'start'",
    "deploy": "npx pm2 --name codetree-prod start npm -- run 'start:deploy' -i 0"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "body-parser": "^1.19.2",
    "compile-run": "^2.3.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "firebase": "^9.6.7",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.16.0",
    "https": "^1.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "3.0.0-beta.9",
    "nodemon": "^2.0.12",
    "openai": "^2.0.5",
    "wordpos": "^2.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.60",
    "@types/python-shell": "^0.4.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-functions-test": "^0.1.6",
    "pm2": "^5.2.0",
    "prettier": "2.5.1",
    "tslint": "^5.12.0",
    "typescript": "^4.9.3",
    "ts-node": "^10.2.1"
  },
  "private": true
}
